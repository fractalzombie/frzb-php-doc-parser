{
    "name": "frzb/php-doc-reader",
    "type": "library",
    "keywords": [
        "library",
        "symfony",
        "component",
        "dependency",
        "injection",
        "reflection",
        "phpdoc"
    ],
    "homepage": "https://github.com/fractalzombie/frzb-php-doc-parser",
    "license": "MIT",
    "authors": [
        {
            "name": "Mykhailo Shtanko",
            "email": "fractalzombie@gmail.com"
        }
    ],
    "description": "PhpDocReader parses @var and @param types",
    "autoload": {
        "psr-4": {
            "FRZB\\Component\\PhpDocReader\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FRZB\\Component\\PhpDocReader\\Tests\\": "Tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "doctrine/annotations": "^1.13",
        "friendsofphp/php-cs-fixer": "^3.9",
        "frzb/dependency-injection": "^1.2",
        "symfony/framework-bundle": "^5.4|^6",
        "fp4php/functional": "^4.18"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mnapoli/hard-mode": "~0.3.0",
        "sempro/phpunit-pretty-print": "^1.4",
        "phpunit/php-code-coverage": "^9.2",
        "php-coveralls/php-coveralls": "^2.5",
        "dg/bypass-finals": "^1.3",
        "symfony/phpunit-bridge": "^5.4|^6",
        "symfony/test-pack": "^1.0"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
