{
    "name": "frzb/php-doc-reader",
    "type": "library",
    "keywords": [
        "library",
        "symfony",
        "component",
        "dependency",
        "injection",
        "reflection",
        "phpdoc"
    ],
    "homepage": "https://github.com/fractalzombie/frzb-php-doc-parser",
    "license": "MIT",
    "authors": [
        {
            "name": "Mykhailo Shtanko",
            "email": "fractalzombie@gmail.com"
        }
    ],
    "description": "PhpDocReader parses @var and @param types",
    "autoload": {
        "psr-4": {
            "FRZB\\Component\\PhpDocReader\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FRZB\\Component\\PhpDocReader\\Tests\\": "Tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "doctrine/annotations": "^1.14",
        "friendsofphp/php-cs-fixer": "^3.42",
        "frzb/dependency-injection": "^2.0",
        "symfony/framework-bundle": "^6.0|^7.0",
        "fp4php/functional": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "mnapoli/hard-mode": "~0.3.0",
        "phpunit/php-code-coverage": "^10.1",
        "php-coveralls/php-coveralls": "^2.7",
        "dg/bypass-finals": "^1.5",
        "symfony/phpunit-bridge": "^6.0|^7.0",
        "symfony/test-pack": "^1.0"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
